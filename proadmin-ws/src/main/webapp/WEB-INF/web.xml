<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
 
<web-app>
	<display-name>Proadmin WS</display-name>
	
	<filter>
       <filter-name>DAOFilter</filter-name>
       <filter-class>com.sasd13.proadmin.ws.filter.DAOFilter</filter-class>
    </filter>
   
    <filter-mapping>
       <filter-name>DAOFilter</filter-name>
       <url-pattern>/*</url-pattern>
    </filter-mapping>
    
	<servlet>
		<servlet-name>Configuration</servlet-name>
		<servlet-class>com.sasd13.proadmin.ws.config.Configuration</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
</web-app>